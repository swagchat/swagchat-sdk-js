{
  "name": "swagchat-sdk",
  "version": "0.11.0",
  "description": "Swagchat SDK - TypeScript & JavaScript -",
  "keywords": [
    "swagchat",
    "chat",
    "messaging",
    "sdk",
    "typescript"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build:lib": "rm -rf ./lib && ./node_modules/.bin/tsc --pretty --watch",
    "build:dev": "./node_modules/.bin/watchify src/index.ts -s SwagChat -p [tsify] --verbose -o dist/swagchat-sdk.js",
    "build:prod": "./node_modules/.bin/watchify src/index.ts -s SwagChat -p [tsify] -g [ envify --NODE_ENV production ] -g uglifyify -p bundle-collapser/plugin --verbose -o dist/swagchat-sdk.min.js",
    "build:doc": "./node_modules/.bin/typedoc --out ./doc/ ./src/",
    "publish": "rm -rf doc && rm -rf dist && rm -rf lib && mkdir dist && ./node_modules/.bin/typedoc --out ./doc/ ./src/ && ./node_modules/.bin/tsc && ./node_modules/.bin/browserify src/index.ts -s SwagChat -p [tsify] -o dist/swagchat-sdk.min.js"
  },
  "author": "Betchi <shinichi.minobe@gmail.com>",
  "contributors": [
    {
      "name": "Betchi",
      "email": "shinichi.minobe@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/swagchat/swagchat-sdk-js"
  },
  "license": "MIT",
  "dependencies": {
    "@types/react-router-redux": "5.0.11",
    "@types/redux-logger": "3.0.0",
    "@types/ws": "0.0.39",
    "bufferutil": "^3.0.3",
    "history": "4.7.2",
    "isomorphic-fetch": "2.2.1",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "3.7.1",
    "redux-logger": "3.0.6",
    "redux-saga": "0.15.3",
    "ws": "2.2.2"
  },
  "devDependencies": {
    "@types/history": "4.6.0",
    "@types/isomorphic-fetch": "0.0.34",
    "bundle-collapser": "1.3.0",
    "envify": "4.1.0",
    "ts-loader": "2.3.4",
    "tsify": "3.0.1",
    "tslint": "5.7.0",
    "tslint-loader": "3.5.3",
    "typedoc": "0.8.0",
    "typescript": "2.4.2",
    "utf-8-validate": "^4.0.0",
    "watchify": "3.9.0"
  }
}
