{
  "name": "swagchat-sdk",
  "version": "0.8.3",
  "description": "Open source chat components - swagchat SDK(TypeScript & JavaScript)",
  "keywords": [
    "swagchat",
    "chat",
    "messaging",
    "sdk",
    "typescript"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build:lib": "./node_modules/.bin/tsc --pretty --watch",
    "build:browser": "./node_modules/.bin/watchify src/index.ts -s SwagChat -p [tsify] --verbose -o dist/swagchat-sdk.min.js",
    "build:doc": "./node_modules/.bin/typedoc --out ./doc/ ./src/"
  },
  "author": "Betchi <shinichi.minobe@gmail.com>",
  "contributors": [
    {
      "name": "Betchi",
      "email": "shinichi.minobe@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/fairway-corp/swagchat-sdk"
  },
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "ws": "^2.2.1"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.34",
    "@types/node": "^8.0.8",
    "@types/ws": "0.0.39",
    "ts-loader": "^2.1.0",
    "tsify": "^3.0.1",
    "tslint": "^5.3.2",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.5.7",
    "typescript": "^2.3.3",
    "watchify": "^3.9.0"
  },
  "optionalDependencies": {
    "bufferutil": "^3.0.0",
    "utf-8-validate": "^3.0.1"
  }
}
